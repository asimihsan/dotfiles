<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.chrony.chronyd</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/sbin/chronyd</string>
        <string>-n</string>
        <string>-f</string>
        <string>/etc/chrony.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardErrorPath</key>
    <string>/var/log/chrony/chronyd.err</string>
    <key>StandardOutPath</key>
    <string>/var/log/chrony/chronyd.out</string>
</dict>
</plist>
